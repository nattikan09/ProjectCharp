<Window x:Class="LectureHashmap.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LectureHashmap"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <TextBox x:Name="textBoxValue" HorizontalAlignment="Left" Height="23" Margin="135,86,0,0" TextWrapping="Wrap" Text="Value" VerticalAlignment="Top" Width="203"/>
        <TextBox x:Name="textBoxKey" HorizontalAlignment="Left" Height="23" Margin="135,55,0,0" TextWrapping="Wrap" Text="Key" VerticalAlignment="Top" Width="200"/>
        <Button x:Name="AddBtn" Content="Add" HorizontalAlignment="Left" Margin="73,152,0,0" VerticalAlignment="Top" Width="75" Click="AddBtn_Click"/>
        <Button x:Name="RemoveBtn" Content="Remove" HorizontalAlignment="Left" Margin="187,152,0,0" VerticalAlignment="Top" Width="75" Click="RemoveBth_Click"/>
        <Button x:Name="ShowBtn" Content="Show All" HorizontalAlignment="Left" Margin="301,151,0,0" VerticalAlignment="Top" Width="75" Click="ShowBtn_Click"/>

    </Grid>
</Window>
